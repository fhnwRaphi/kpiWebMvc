
<script>
    $(document).ready(function () {
        var sitenameHome = "Home";

        $(".myHomeNavBtn").click(function () {
            sitenameHome = $(this).attr("id");
            var partialContent = $("#partialContent");
            $.ajax({
                type: "GET",
                url: '/Home/' + sitenameHome,
                data: { startDate: "", stopDate: "" },
                success: function (data) {
                    //partialContent.html('');
                    partialContent.html(data);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert('error on load.');
                }
            });
        });
    });
</script>
